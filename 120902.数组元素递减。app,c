#include<stdio.h>
void main() {
	int a = 90;
	int i,j;
	int array[9][10];{
	for(i = 0; i < 9; i++) 
		for( j = 0; j < 10; j++) {
			array[i][j] = 0;
			printf("%d ", array[i][j]);
		}
		printf("\n");
	}
for(i =0; i<9; i++) {
		for( j=0; j<10; j++) {
			array[i][j] = a--;
			printf("%d", array[i][j]);
		}
		printf("\n");
}
}
